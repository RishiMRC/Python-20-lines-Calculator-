def cal (a,b):
    if '+' in op:
        print (a+b)
    elif '×'  in op:
        print (a*b)
    elif '-'  in op:
        print (a-b)
    elif '÷' in op:
        print (a/b)
    else:
        print ("Invalid")     
        
